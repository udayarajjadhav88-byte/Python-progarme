    "def attendance(time):\n",
    "    if \"9:30\"==time:\n",
    "        print(\"student is no time and 'present'\")\n",
    "    elif\"10:00\"<time:\n",
    "        print(\"student is 'late'\")\n",
    "    else:\n",
    "        print(\"student is not on time 'absent'\")\n",
    "time=input(\"enter your time for entry:\")\n",
    "attendance(time)"
